{
    "require": {
        "bryanjhv/slim-session": "^3.3",
        "codeguy/upload": "^1.3",
        "fergusean/nusoap": "^0.9.5",
        "gregwar/captcha": "^1.1",
        "nategood/httpful": "^0.2.20",
        "phpfastcache/phpfastcache": "^5.0",
        "predis/predis": "^1.1",
        "slim/csrf": "^0.8.2",
        "slim/middleware": "^1.0",
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.2",
        "zircote/swagger-php": "^2.0"
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false,
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "app\\": "app/",
            "console\\": "console/"
        },
        "files": ["app/lib/helper.php"]
    },
    "require-dev": {
        "bamboohr/phpcs": "^1.0",
        "codedungeon/phpunit-result-printer": "^0.5.4",
        "friendsofphp/php-cs-fixer": "2.2.5",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "4.8.36",
        "sebastian/phpcpd": "2.0.4"
    }
}
